SELECT a.full_name, s.hometown, s.spouse FROM ages_stats a
    JOIN stats s
    ON a.id = s.id;
    
SELECT a.full_name, a.age, a.zodiac_sign, e.zodiac_element FROM ages_stats a
    JOIN elements e
    ON a.id = e.id;

SELECT zodiac_element FROM elements
